#ifndef HUMMSTRUMM_COMPILECONFIG
#define HUMMSTRUMM_COMPILECONFIG

// Humm and Strumm version
#cmakedefine HUMMSTRUMM_VERSION "@HUMMSTRUMM_VERSION@"
#cmakedefine HUMMSTRUMM_VERSION_NUMBER "@HUMMSTRUMM_VERSION_NUMBER@"

// Platforms
#cmakedefine HUMMSTRUMM_PLATFORM_WINDOWS
#cmakedefine HUMMSTRUMM_PLATFORM_GNULINUX
#cmakedefine HUMMSTRUMM_PLATFORM_BSD
#cmakedefine HUMMSTRUMM_ARCHITECTURE_64
#cmakedefine HUMMSTRUMM_ARCHITECTURE_32

#cmakedefine HUMMSTRUMM_WINDOWSYSTEM_WINDOWS
#cmakedefine HUMMSTRUMM_WINDOWSYSTEM_X11

#cmakedefine HUMMSTRUMM_HAVE_SSE_SUPPORT

// File name of the log.
#cmakedefine HUMMSTRUMM_LOG_FILENAME "@HUMMSTRUMM_LOG_FILENAME@"
// Minimum level of log messages to output.
#cmakedefine HUMMSTRUMM_LOG_LOGLEVEL LEVEL_@HUMMSTRUMM_LOG_LOGLEVEL@

// Debug mode?
#cmakedefine HUMMSTRUMM_DEBUG

#endif // #ifndef HUMMSTRUMM_CONFIG
